def main():
    s = []

    s1 = string.ascii_uppercase
    s2 = string.ascii_lowercase
    s3 = string.digits
    s4 = string.punctuation


    s.extend(s1)
    s.extend(s2)
    s.extend(s3)
    s.extend(s4)

    random.shuffle(s)

    plen = int(input("Enter Password Leangth: "))

    print("".join(s[0:plen]))

main()
